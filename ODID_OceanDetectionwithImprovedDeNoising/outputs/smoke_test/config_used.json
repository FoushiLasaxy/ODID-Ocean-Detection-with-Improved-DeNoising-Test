{
  "seed": 42,
  "device": "cpu",
  "data": {
    "name": "fake",
    "data_dir": "./data",
    "imagefolder_train_dir": "",
    "imagefolder_val_dir": "",
    "num_classes": 10,
    "img_size": 32,
    "num_workers": 0,
    "pin_memory": true,
    "download": true,
    "fake_train_size": 512,
    "fake_val_size": 128,
    "normalize": true
  },
  "model": {
    "name": "simple_cnn",
    "pretrained": false,
    "dropout": 0.0
  },
  "train": {
    "epochs": 1,
    "batch_size": 16,
    "lr": 0.001,
    "weight_decay": 0.0005,
    "log_interval": 20,
    "label_smoothing": 0.0,
    "amp": false,
    "grad_clip_norm": 0.0
  },
  "optimizer": {
    "name": "adamw",
    "momentum": 0.9
  },
  "scheduler": {
    "name": "cosine",
    "warmup_epochs": 0,
    "min_lr": 1e-06,
    "step_size": 10,
    "gamma": 0.1
  },
  "output": {
    "root_dir": "./outputs",
    "exp_name": "smoke_test",
    "save_best_name": "best.pt",
    "save_last_name": "last.pt",
    "log_name": "train.log",
    "metrics_name": "metrics.csv"
  }
}